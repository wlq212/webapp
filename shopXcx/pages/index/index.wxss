/**index.wxss**/

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
button{
background-color: #fff;
}
button::after{
  border: 0px;
  
  
  background-size: 100%;
}
.userinfo-avatar {
  width: 128rpx;
