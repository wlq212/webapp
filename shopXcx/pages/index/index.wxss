@keyframes slidown {
  from {
    height: 0rpx;
  }
 
  to {
    height: 200rpx;
  }
}
 
.slidown {
  display: block;
  animation: slidown 0.3s linear both;
}
 
@keyframes slidup {
  from {
    height: 200rpx;
  }
 
  to {
    height: 0rpx;
  }
}
 
.slidup {
  display: block;
  animation: slidup 0.3s linear both;
}
