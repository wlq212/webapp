  margin-top: 10rpx;
  
}
.servicePrice{
  font-size:32rpx;
  color:rgb(229,97,98);
  font-family:PingFangSC-Semibold;
  margin-top: 0rpx;
  margin-left: 16rpx;
}
.title{
  overflow: hidden;
  width:690rpx;
  margin: 0 auto;
  margin-top:26rpx; 
}

.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom: 20rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;  
  width: 14rpx;  
  height: 14rpx;  
  background:rgba(255,255,255,0.5);  
  border-radius: 50%;  
  transition: all .6s; 
}  
.swiper-container .dots .dot.active{  
  width: 14rpx;  
  background: #fff;
  opacity: 1;
}
.serviceInfo{
  width:280rpx;
  height: 310rpx;
  background: url("data:image/png;base64,") no-repeat;
