} 
.right button{
  background: #fff;
  margin-top:5rpx;
  padding-right: 0rpx;
  font-size:22rpx;
  font-family:PingFangSC-Regular;color:rgb(83,83,83);
}
.container{
  width:750rpx;
  margin:  0 auto;
  height: 1100rpx;
  
}
.citySeletc{
  padding:10rpx 0rpx;
}
.serviceName{
  font-size:24rpx;
  color:rgb(83,83,83);
  font-family:PingFangSC-Regular;
