}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.left{
  float: left;
  font-size:32rpx;
  color: rgb(9,9,9);
  font-family: PingFangSC-Semibold;
}
.right{
  float: right;
  color: #ddd;
